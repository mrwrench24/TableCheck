   },\n    addParty: (name, size, phoneNumber, quotedTime) => {\n      setWaitlist(\n        [\n          ...waitlist,\n          {\n            name,\n            size,\n            phoneNumber,\n            quotedTime,\n            key: Math.random() * 1000\n          }\n        ]\n      )\n    },\n    getValueFrom: (value, targetNumber) => {\n      return waitlist.map( (party) => {\n        if (party.phoneNumber === targetNumber) {\n          return getValueFrom(value, party);\n        }\n      })\n    }\n  };\n\n  return (\n    <WaitlistContext.Provider value={valueToShare}>\n      {children}\n    </WaitlistContext.Provider>\n  );\n}\n\nexport { Provider };\nexport default WaitlistContext;\n","/Users/jakesquatrito/tablecheck/src/components/WaitingList.js",[],[],"/Users/jakesquatrito/tablecheck/src/components/Button.js",[],[],"/Users/jakesquatrito/tablecheck/src/components/SummaryParty.js",[],[],"/Users/jakesquatrito/tablecheck/src/components/Dropdown.js",[],[],"/Users/jakesquatrito/tablecheck/src/components/desktop/WaitingParty.js",[],[],"/Users/jakesquatrito/tablecheck/src/components/desktop/EditParty.js",[],[],"/Users/jakesquatrito/tablecheck/src/components/phone/WaitingPartyPhone.js",[],[],"/Users/jakesquatrito/tablecheck/src/components/phone/EditPartyPhone.js",[],[],"/Users/jakesquatrito/tablecheck/src/components/CreateParty.js",[],[],"/Users/jakesquatrito/tablecheck/src/components/CreateModal.js",[],[],{"ruleId":"70","severity":1,"message":"71","line":84,"column":36,"nodeType":"72","messageId":"73","endLine":84,"endColumn":38},"array-callback-return","Array.prototype.map() expects a value to be returned at the end of arrow function.","ArrowFunctionExpression","expectedAtEnd"]